{
	"name": "vscode-scss-lang",
	"displayName": "SCSS Language Support",
	"description": "Rich language support for SCSS",
	"version": "0.1.3",
	"author": "Danny McGee <dannymcgee@gmail.com>",
	"publisher": "dannymcgee",
	"license": "MIT",
	"keywords": ["scss", "sass", "syntax", "grammar", "language"],
	"icon": "icon.png",
	"engines": {
		"vscode": "^1.22.0"
	},
	"repository": {
		"types": "git",
		"url": "https://github.com/dannymcgee/vscode-scss"
	},
	"contributes": {
		"languages": [
			{
				"id": "scss-parser-snap",
				"aliases": ["SCSS Parser Snapshot"],
				"extensions": [".parser-snap"]
			}
		],
		"grammars": [
			{
				"language": "scss",
				"scopeName": "source.scss",
				"path": "./scss.tmLanguage.json"
			},
			{
				"language": "scss-parser-snap",
				"scopeName": "source.parser-snap",
				"path": "./parser-snap.tmLanguage.json"
			}
		]
	}
}
